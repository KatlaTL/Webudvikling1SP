<main class="container">
  <h2>Feature requests</h2>
  <a href="/featureRequest/create">New feature request</a>
  <hr>
  <div class="list">
    <% for(let index in featureRequest) { %>
      <div class="list__item">
        <h3><a href="/featureRequest/<%= featureRequest[index].id %>"><%= featureRequests[index].title %></h3></a>
        <p><%= featureRequests[index].author %></p>
      </div>
    <% } %>
  </div>
</main>